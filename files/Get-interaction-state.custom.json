{
  "name": "Get interaction state - Exported 2021-04-15 @ 18:57",
  "integrationType": "purecloud-data-actions",
  "actionType": "custom",
  "config": {
    "request": {
      "requestUrlTemplate": "/api/v2/conversations/${input.conversationId}",
      "requestType": "GET",
      "headers": {},
      "requestTemplate": "${input.rawRequest}"
    },
    "response": {
      "translationMap": {
        "state": "$.participants[0].callbacks[0].state"
      },
      "translationMapDefaults": {
        "state": "disconnected"
      },
      "successTemplate": "{\"state\": ${state}}"
    }
  },
  "contract": {
    "input": {
      "inputSchema": {
        "type": "object",
        "properties": {
          "conversationId": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    },
    "output": {
      "successSchema": {
        "type": "object",
        "properties": {
          "state": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    }
  },
  "secure": false
}